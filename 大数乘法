高精度乘法
#include<iostream>
#include<vector>
using namespace std;
vector<int> Mul(vector<int>& A, int b)
{
	vector<int> C;

	int t = 0;
	for (int i = 0; i < A.size() || t; i++)
	{
		if (i < A.size()) t += A[i] * b;
		C.push_back(t % 10);
		t /= 10;
	}

	while (C.size() > 1 && C.back() == 0) C.pop_back();

	return C;
}
int main()
{
	string a;
	vector<int>A, C; int b;
	cin >> a >> b;
	if (b == 0)
		cout << '0';
	else if (b == 1)
		cout << a;
	else 
	{
		for (int i = a.size() - 1; i >= 0; i--)A.push_back(a[i] - '0');
		C = Mul(A, b);
		for (int i = C.size() - 1; i >= 0; i--)cout <<C[i];
	}		
}
