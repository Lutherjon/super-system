#include<iostream>
const int N=1010;
int a[N][N],s[N][N];
int main()
{
    int n,m,i,j,q;
    scanf("%d%d",&n,&m,&q);
    s[0][0]=0;
    for(i=1; i<=n; i++)
        for(j=1; j<=m; j++)
            scanf("%d",&a[i][j]);
    for(i=1; i<=n; i++)
        for(j=1; j<=m; j++)
            s[i][j]=s[i][j-1]+s[i-1][j]+a[i][j]-s[i-1][j-1];
    int l1,r1,l2,r2;
    while(q--) {
        scanf("%d%d",&l1,&r1,&l2,&r2);
        if(l1>l2)
            l1^=l2^=l1^=l2;
        if(r1>r2)
            r1^=r2^=r1^=r2;
        printf("%d",s[l2][r2]+s[l1-1][r1-1]-s[l2][r1-1]-s[l-1][r2]);
    }
}
    
